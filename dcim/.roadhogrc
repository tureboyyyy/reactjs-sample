{
  "entry": "src/index.js",
  "proxy": {
    	  "/api": {
    		"target": "http://localhost:3003/",
    		"changeOrigin": true,
    		"pathRewrite": { "^/api" : "" }
    	  }
    	},
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
